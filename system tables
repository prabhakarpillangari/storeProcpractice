use Indian_Bank
go

select * from sys.tables

select count(*) from sys.columns 
where object_id =581577110

select count(*) from sys.columns
where object_id=(
				select object_id from sys.tables 
				where name ='BranchMaster')


---check status column is availble or not?
select * from sys.columns
where name ='status'


----columname 
----identify the table
select a.name as tableName, b.name as ColName
from sys.tables a join sys.columns as b
on a.object_id=b.object_id
where b.name='cbalance'

select * from sys.columns
 
if Exists(select * from sys.tables where name ='A')
		drop table A
create table A
(
number int
)

insert into A values (1),(2),(3),(null)
select * from A


